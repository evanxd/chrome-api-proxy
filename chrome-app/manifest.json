{
  "manifest_version": 2,
  "name": "Chrome API Proxy",
  "description": "Call Chrome App APIs from a Webpage.",
  "version": "0.0.1",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "externally_connectable": {
    "matches": ["http://localhost:8080/*"]
  },
  "permissions": [
    "alwaysOnTopWindows",
    "serial"
  ]
}
